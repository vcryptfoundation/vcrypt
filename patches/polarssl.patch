+/* returns number of bytes needed to encode this length */
+int asn1_encode_len(int len, uint8 *buff, int bs)
+{
+	assert(len < 4096);
+
+	uint8 *b = buff;
+
+	int lenlen = get_len_len(len);
+	if (lenlen > bs)
+		return 0;
+
+	if (lenlen == 0)
+		return 0;
+
+	if (lenlen == 1) {
+		*buff = len;
+		return 1;
+	}
+
+	*b++ = 0x80 | (lenlen - 1);
+
+	int i;
+	for (i = 0; i < lenlen - 1; i++) {
+		*b++ = ((uint8*) &len)[lenlen - 2 - i];
+	}
+
+	return lenlen;
+}
+
+asn1_chunk* asn1_new_chunk(asn1_chunk *parent, int type, int len, void *buff)
+{
+	asn1_chunk *chunk = NULL;
+
+	// sanity checks
+	if (len >= 32700 || len < 0)
+		goto ret_err;
+
+	// we support only 1-byte types
+	if (type > 255 || type < 0)
+		goto ret_err;
+
+	// data buff/len inconsistency
+	if ((len == 0) != (buff == 0))
+		goto ret_err;
+
+	// we can't specify data for constructed/encapsulated tags
+	if ((type & ASN1_CONSTRUCTED) && (len || buff))
+		goto ret_err;
+
+	// check if the parent can have children, TODO: add encapsulation possibility
+	if (parent && ((parent->type & ASN1_CONSTRUCTED) == 0))
+		goto ret_err;
+
+	chunk = calloc(1, sizeof *chunk);
+	if (!chunk)
+		goto ret_err;
+
+	// all lenghts of primitives are calculated at append time
+	chunk->type = type;
+	chunk->data = NULL;
+	chunk->n_children = 0;
+	chunk->children = NULL;
+
+	if (type & ASN1_CONSTRUCTED) {
+		// length will be calculated at encode time, according to the length of all children
+		chunk->data_len = 0;
+		chunk->children = calloc(1, MAX_CHILDREN);
+	}
+
+	if (len) {
+		chunk->data_len = len;
+		chunk->data = calloc(1, len);
+		if (chunk->data == NULL )
+			goto ret_err;
+
+		memcpy(chunk->data, buff, len);
+	}
+
+	if (parent) {
+		if (parent->n_children == MAX_CHILDREN)
+			goto ret_err;
+
+		parent->n_children++;
+		parent->children[parent->n_children - 1] = chunk;
+	}
+
+	return chunk;
+
+	ret_err: //
+	if (chunk) {
+		if (chunk->children)
+			free(chunk->children);
+
+		if (chunk->data)
+			free(chunk->data);
+
+		free(chunk);
+	}
+	assert(2 == 3); // make it crash here
+	return NULL ;
+}
+
+int asn1_free_chunk(asn1_chunk **chunk)
+{
+	if (*chunk == NULL )
+		return 0;
+
+	int i;
+	for (i = 0; i < (*chunk)->n_children; i++) {
+		asn1_free_chunk(&(*chunk)->children[i]);
+	}
+
+	if ((*chunk)->children) {
+		free((*chunk)->children);
+		(*chunk)->children = NULL;
+	}
+
+	if ((*chunk)->data) {
+		free((*chunk)->data);
+		(*chunk)->data = NULL;
+	}
+
+	(*chunk)->n_children = 0;
+	free(*chunk);
+	*chunk = NULL;
+
+	return 0;
+}
+
+int depth;
+int asn1_update_len(asn1_chunk *chunk)
+{
+	int i;
+	for (i = 0; i < chunk->n_children; i++) {
+		int tmplen = asn1_update_len(chunk->children[i]);
+
+		if (tmplen == 0)
+			return 0;
+
+		chunk->data_len += tmplen;
+	}
+
+	int ret = chunk->data_len + get_len_len(chunk->data_len)
+			+ get_type_len(chunk->type);
+
+	return ret;
+}
+
+int asn1_encode(asn1_chunk *chunk, uint8 *start, uint8 *end)
+{
+	uint8 *s = start;
+	depth++;
+
+	int enc_len;
+	if (!(enc_len = asn1_encode_type(chunk->type, s, end - s)))
+		goto ret;
+	s += enc_len;
+
+	if (!(enc_len = asn1_encode_len(chunk->data_len, s, end - s)))
+		goto ret;
+	s += enc_len;
+
+	if (chunk->data && chunk->data_len) {
+		if (chunk->data_len >= (end - s))
+			goto ret;
+
+		memcpy(s, chunk->data, chunk->data_len);
+		s += chunk->data_len;
+	} else {
+		int i;
+		for (i = 0; i < chunk->n_children; i++) {
+			asn1_chunk *c = chunk->children[i];
+
+			// check some sanity
+			assert(c);
+
+			if (c->type & ASN1_CONSTRUCTED) {
+				assert(chunk->data == NULL);
+			}
+
+			enc_len = asn1_encode(c, s, end);
+			if (enc_len == 0)
+				goto ret;
+
+			assert(enc_len);
+			s += enc_len;
+		}
+	}
+
+	depth--;
+
+	//assert(s - start == chunk->data_len);
+	return s - start;
+
+	ret: //
+	depth--;
+	return 0;
+}
+
+static void add_keysig_algo(asn1_chunk* cert_tbs, char *algo)
+{
+	//	signature AlgorithmIdentifier,
+	asn1_chunk *cert_tbs_algo = asn1_new_chunk(cert_tbs, ASN1_SEQ, 0, 0);
+	asn1_new_chunk(cert_tbs_algo, ASN1_OID, 9, algo);
+	asn1_new_chunk(cert_tbs_algo, ASN1_NULL, 0, 0);
+}
+
+static void make_utctime(char *timestr, int timestr_size, struct tm* utc)
+{
+	const char* utc_format = "%02d%02d%02d%02d%02d%02dZ";
+	snprintf(timestr, timestr_size, utc_format, (utc->tm_year + 1900) % 100,
+			utc->tm_mon + 1, utc->tm_mday, utc->tm_hour, utc->tm_min,
+			utc->tm_sec);
+}
+
+static void add_validity(asn1_chunk* parent)
+{
+	asn1_chunk *cert_validity = asn1_new_chunk(parent, ASN1_SEQ, 0, 0);
+
+	time_t unixtime;
+	struct tm utc;
+	char timestr[14];
+
+	unixtime = time(NULL );
+	unixtime -= 60;
+	gmtime_r(&unixtime, &utc);
+
+	make_utctime(timestr, 14, &utc);
+	asn1_new_chunk(cert_validity, ASN1_UTC_TIME, 13, timestr);
+
+	unixtime += 60 * 60 * 24 * 30;
+	gmtime_r(&unixtime, &utc);
+	make_utctime(timestr, 14, &utc);
+
+	asn1_new_chunk(cert_validity, ASN1_UTC_TIME, 13, timestr);
+}
+
+void add_name(asn1_chunk* parent)
+{
+	asn1_chunk *cert_issuer = asn1_new_chunk(parent, ASN1_SEQ, 0, 0);
+	asn1_chunk* cert_issuer_set;
+	asn1_chunk* cert_issuer_seq;
+
+	char *oids[] = {OID_COUNTRY, OID_CN, OID_ORGANIZATION, NULL };
+	char *values[] = {"US", "Vcrypt", "Vcrypt Org", NULL };
+
+	char **o = oids;
+	char **v = values;
+
+	while (*o) {
+		cert_issuer_set = asn1_new_chunk(cert_issuer,
+				ASN1_SET | ASN1_CONSTRUCTED, 0, 0);
+
+		cert_issuer_seq = asn1_new_chunk(cert_issuer_set, ASN1_SEQ, 0, 0);
+		asn1_new_chunk(cert_issuer_seq, ASN1_OID, 3, *o);
+		asn1_new_chunk(cert_issuer_seq, ASN1_PRINTABLE_STRING, strlen(*v), *v);
+
+		o++;
+		v++;
+	}
+}
+
+asn1_chunk* asn1_new_chunk_mpi(asn1_chunk* parent, mpi* mpi_src)
+{
+	size_t mpisize = mpi_size(mpi_src);
+
+	unsigned char *mpib = alloca(mpisize + 1);
+	assert(mpib);
+
+	mpib[0] = 0; // for padding
+
+	if (mpi_write_binary(mpi_src, mpib + 1, mpisize))
+		return NULL ;
+
+	unsigned char *source;
+	int size;
+
+	// now check if the padding is really needed
+	if (mpisize > 1 && (mpib[1] & 0x80)) {
+		source = mpib;
+		size = mpisize + 1;
+	} else {
+		source = mpib + 1;
+		size = mpisize;
+	}
+
+	return asn1_new_chunk(parent, ASN1_INTEGER, size, source);
+}
+
+int add_public_key(asn1_chunk* cert_tbs, mpi* pub_key_N, mpi* pub_key_E)
+{
+	// prepare bitstring value for public key
+	asn1_chunk *cert_pk_seq = asn1_new_chunk(NULL, ASN1_SEQ, 0, 0);
+
+	asn1_new_chunk_mpi(cert_pk_seq, pub_key_N);
+	asn1_new_chunk_mpi(cert_pk_seq, pub_key_E);
+
+	uint8 pkbitstr[1024];
+	pkbitstr[0] = 0;
+	asn1_update_len(cert_pk_seq);
+	int bslen = asn1_encode(cert_pk_seq, pkbitstr + 1,
+			pkbitstr + sizeof(pkbitstr) - 1);
+
+	asn1_free_chunk(&cert_pk_seq);
+
+	// subjectPublicKeyInfo
+	asn1_chunk *cert_pk = asn1_new_chunk(cert_tbs, ASN1_SEQ, 0, 0);
+	add_keysig_algo(cert_pk, OID_PKCS1_RSA);
+	asn1_new_chunk(cert_pk, ASN1_BIT_STRING, bslen + 1, pkbitstr);
+
+	return 0;
+}
+
+int x509_encode_fake_cert(mpi *pub_key, mpi *pub_key_exp, unsigned char *buff,
+		int buflen)
+{
+	/*
+	 * Certificate  ::=  SEQUENCE  {
+	 *      tbsCertificate       TBSCertificate,
+	 *      signatureAlgorithm   AlgorithmIdentifier,
+	 *      signatureValue       BIT STRING  }
+	 *
+	 */
+
+	asn1_chunk *cert = asn1_new_chunk(NULL, ASN1_SEQ, 0, 0);
+	asn1_chunk *cert_tbs = asn1_new_chunk(cert, ASN1_SEQ, 0, 0);
+
+	uint8 temp[4] = {0, 1};
+	add_keysig_algo(cert, OID_PKCS1_SHA1);
+
+	// fake signature
+	asn1_new_chunk(cert, ASN1_BIT_STRING, 2, &temp);
+
+	// setup TBS
+	/*TBSCertificate ::= SEQUENCE {
+	 version [0] EXPLICIT Version DEFAULT v1,
+	 serialNumber CertificateSerialNumber,
+	 signature AlgorithmIdentifier,
+	 issuer Name,
+	 validity Validity,
+	 subject Name,
+	 subjectPublicKeyInfo SubjectPublicKeyInfo
+	 }*/
+
+	// version
+	asn1_chunk *cert_tbs_ver_tag = asn1_new_chunk(cert_tbs,
+			ASN1_CONSTRUCTED | ASN1_CONTEXT_SPECIFIC, 0, 0);
+	temp[0] = 2;
+	asn1_new_chunk(cert_tbs_ver_tag, ASN1_INTEGER, 1, temp);
+
+	// fake serial number
+	temp[0] = 1;
+	asn1_new_chunk(cert_tbs, ASN1_INTEGER, 1, temp);
+
+	//	signature AlgorithmIdentifier,
+	add_keysig_algo(cert_tbs, OID_PKCS1_SHA1);
+
+	// issuer Name,
+	add_name(cert_tbs);
+
+	// validity Validity,
+	add_validity(cert_tbs);
+
+	// subject Name,
+	add_name(cert_tbs);
+
+	// subjectPublicKeyInfo
+	add_public_key(cert_tbs, pub_key, pub_key_exp);
+
+	int enc_len = asn1_update_len(cert);
+
+	depth = 0;
+	enc_len = asn1_encode(cert, buff, buff + buflen);
+
+	asn1_free_chunk(&cert);
+
+//	// save stuff
+//	FILE *f = fopen("asn.der", "w");
+//	fwrite(buff, 1, enc_len, f);
+//	fclose(f);
+
+	return enc_len;
+}
+
+int asn1_encode_private_key_der(unsigned char *dest, int destlen,
+		rsa_context *rsa)
+{
+	asn1_chunk *seq = asn1_new_chunk(NULL, ASN1_SEQ, 0, 0);
+
+	char test = 0;
+	asn1_new_chunk(seq, ASN1_INTEGER, 1, &test);
+
+	asn1_new_chunk_mpi(seq, &rsa->N);
+	asn1_new_chunk_mpi(seq, &rsa->E);
+	asn1_new_chunk_mpi(seq, &rsa->D);
+	asn1_new_chunk_mpi(seq, &rsa->P);
+	asn1_new_chunk_mpi(seq, &rsa->Q);
+	asn1_new_chunk_mpi(seq, &rsa->DP);
+	asn1_new_chunk_mpi(seq, &rsa->DQ);
+	asn1_new_chunk_mpi(seq, &rsa->QP);
+
+	int enc_len = asn1_update_len(seq);
+
+	enc_len = asn1_encode(seq, dest, dest + destlen);
+
+	asn1_free_chunk(&seq);
+
+	return enc_len;
+}
+
+int asn1_encode_public_key_der(unsigned char *dest, int destlen,
+		rsa_context *rsa)
+{
+	asn1_chunk *seq = asn1_new_chunk(NULL, ASN1_SEQ, 0, 0);
+
+	asn1_new_chunk_mpi(seq, &rsa->N);
+	asn1_new_chunk_mpi(seq, &rsa->E);
+
+	int enc_len = asn1_update_len(seq);
+	enc_len = asn1_encode(seq, dest, dest + destlen);
+	asn1_free_chunk(&seq);
+
+	return enc_len;
+}
+
+int asn1_encode_private_key_der_file(char *fname, rsa_context *rsa)
+{
+	unsigned char buff[4096];
+
+	int enc_len = asn1_encode_private_key_der(buff, sizeof(buff), rsa);
+
+	if (enc_len < 0)
+		return enc_len;
+
+	FILE *f = fopen(fname, "wb");
+	if (f == NULL )
+		return -1;
+
+	int write_len = fwrite(buff, 1, enc_len, f);
+
+	fclose(f);
+
+	return write_len == enc_len ? 0 : -1;
+
+}
