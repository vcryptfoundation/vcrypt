+#include "polarssl/rsa.h"
+#include "polarssl/net.h"
+#include "polarssl/x509.h"
+#include "polarssl/error.h"
+
+typedef struct ssl_prerequisites {
+	rsa_context rsa;
+	x509_cert srvcert;
+	entropy_context entropy;
+	ctr_drbg_context ctr_drbg;
+} ssl_prerequisites;
+
+void ssl_requisites_free(ssl_prerequisites *req)
+{
+	rsa_free(&req->rsa);
+	x509_free(&req->srvcert);
+}
+
+int ssl_requisites_init(ssl_prerequisites* req)
+{
+	int ret;
+	memset(req, 0, sizeof *req);
+	memset(&req->srvcert, 0, sizeof req->srvcert); // this is mandatory!
+
+	rsa_init(&req->rsa, RSA_PKCS_V15, SIG_RSA_SHA384);
+	entropy_init(&req->entropy);
+
+	char* pers = "ssl_server";
+	if ((ret = ctr_drbg_init(&req->ctr_drbg, entropy_func, &req->entropy,
+			(const unsigned char*) pers, strlen(pers))) != 0) {
+		printf("ERROR: Failed initializing random number generator: %d\n", ret);
+		return ret;
+	}
+
+	return 0;
+}
+
+#define DEBUG_LEVEL 0
+
+void my_ssl_debug(void *ctx, int level, const char *str)
+{
+	if (level < DEBUG_LEVEL) {
+		fprintf((FILE *) ctx, "%s", str);
+		fflush((FILE *) ctx);
+	}
+}
+
+typedef struct CLIENT_CONNDATA {
+	struct sockaddr_in cl_addr;
+	uint32_t p2p_port;
+} CLIENT_CONNDATA;
+
+/*
+ * connect to this server with:
+ * openssl s_client -tls1_2 -connect localhost:5566
+ * */
+
+int main(void)
+{
+	ssl_prerequisites ssl_req;
+	ssl_context ssl;
+	int socket_server = 0;
+	int ret = 0;
+	int srv_port = 5566;
+	const char *srv_hostname = "localhost";
+
+	printf("Initializing SSL requisites .. ");
+	if ((ret = ssl_requisites_init(&ssl_req)) != 0) {
+		printf("ERROR\n");
+		goto exit;
+	}
+	printf("OK\n");
+
+	printf("Initializing SSL ... ");
+	if ((ret = ssl_init(&ssl)) != 0) {
+		printf("ERROR\n");
+		goto exit;
+	}
+	printf("OK\n");
+
+	printf("Connecting ... ");
+	if ((ret = net_connect(&socket_server, srv_hostname, srv_port)) != 0) {
+		printf("ERROR\n");
+		goto exit;
+	}
+
+	printf("OK\n");
+
+	ssl_set_endpoint(&ssl, SSL_IS_CLIENT);
+	ssl_set_authmode(&ssl, SSL_VERIFY_NONE);
+	ssl_set_rng(&ssl, ctr_drbg_random, &ssl_req.ctr_drbg);
+	ssl_set_dbg(&ssl, my_ssl_debug, stdout);
+
+	ssl_set_bio(&ssl, net_recv, &socket_server, net_send, &socket_server);
+
+	printf("Switching to non-blocking mode ... ");
+	if ((ret = net_set_block(socket_server)) != 0) {
+		printf("ERROR\n");
+		goto exit;
+	}
+
+	printf("OK\n");
+
+	printf("handshaking... ");
+	while ((ret = ssl_handshake(&ssl)) != 0) {
+		if (ret != POLARSSL_ERR_NET_WANT_READ
+				&& ret != POLARSSL_ERR_NET_WANT_WRITE) {
+			break;
+		}
+	}
+
+	if (ret) {
+		printf("ERROR\n");
+		goto exit;
+	} else {
+		printf("OK\n");
+	}
+
+	printf("Writing data ... ");
+	char *towrite = "write this string";
+	while (1) {
+		ret = ssl_write(&ssl, (unsigned char *) towrite, sizeof towrite);
+		if (ret == POLARSSL_ERR_NET_WANT_WRITE)
+			continue;
+
+		if (ret < 0) {
+			printf("Could not send data\n");
+			goto exit;
+		}
+
+		break;
+	}
+
+	if (ret < 0) {
+		printf("ERROR\n");
+		goto exit;
+	} else {
+		printf("OK, wrote %d bytes\n", ret);
+	}
+
+	printf("Receiving data ... ");
+	char read_buff[512];
+	while (1) {
+		ret = ssl_read(&ssl, (unsigned char*) read_buff, sizeof read_buff);
+		if (ret == POLARSSL_ERR_NET_WANT_READ)
+			continue;
+
+		break;
+	}
+
+	if (ret < 0) {
+		printf("ERROR\n");
+		goto exit;
+	} else {
+		printf("OK, read %d bytes\n", ret);
+	}
+
+	printf("Checking if numbers of sent and received bytes match ... %s\n",
+			ret != sizeof towrite ? "ERROR" : "OK");
+
+	printf("Checking if sending and receiving buffers match ... %s\n",
+			memcmp(towrite, read_buff, MIN(sizeof towrite, ret)) ?
+					"ERROR" : "OK");
+
+	if (ret > 0)
+		ret = 0;
+
+	exit: //
+	if (ret) {
+		char errbuff[512];
+		error_strerror(ret, errbuff, sizeof errbuff);
+		printf(" - %s\n", errbuff);
+	}
+
+	printf("Exiting.\n");
+
+	ssl_requisites_free(&ssl_req);
+	ssl_free(&ssl);
+
+	return 0;
+}
