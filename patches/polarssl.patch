+/*
+ * server.c
+ *      Author: 655518d74018d1215d9d5a8597a99cba
+ */
+
+#include <arpa/inet.h>
+#include <errno.h>
+#include <malloc.h>
+#include <netdb.h>
+#include <netinet/in.h>
+#include <resolv.h>
+#include <stdint.h>
+#include <stdlib.h>
+#include <string.h>
+#include <sys/socket.h>
+#include <sys/types.h>
+#include <unistd.h>
+
+#include "polarssl/entropy.h"
+#include "polarssl/ctr_drbg.h"
+#include "polarssl/entropy.h"
+#include "polarssl/ssl.h"
+#include "polarssl/rsa.h"
+#include "polarssl/net.h"
+#include "polarssl/x509.h"
+#include "polarssl/error.h"
+
+typedef struct ssl_prerequisites {
+	rsa_context rsa;
+	x509_cert srvcert;
+	entropy_context entropy;
+	ctr_drbg_context ctr_drbg;
+} ssl_prerequisites;
+
+void ssl_requisites_free(ssl_prerequisites *req)
+{
+	rsa_free(&req->rsa);
+	x509_free(&req->srvcert);
+}
+
+int ssl_requisites_init(ssl_prerequisites* req)
+{
+	int ret;
+	memset(req, 0, sizeof *req);
+	memset(&req->srvcert, 0, sizeof req->srvcert); // this is mandatory!
+
+	rsa_init(&req->rsa, RSA_PKCS_V15, SIG_RSA_SHA384);
+	entropy_init(&req->entropy);
+
+	char* pers = "ssl_server";
+	if ((ret = ctr_drbg_init(&req->ctr_drbg, entropy_func, &req->entropy,
+			(const unsigned char*) pers, strlen(pers))) != 0) {
+		printf("ERROR: Failed initializing random number generator: %d\n", ret);
+		return ret;
+	}
+
+	return 0;
+}
+
+int ssl_load_keys(ssl_prerequisites *req, char *priv_key_fname, char *pass)
+{
+	int ret;
+
+	if ((ret = ssl_requisites_init(req)) != 0)
+		goto exit;
+
+	ret = x509parse_keyfile(&req->rsa, priv_key_fname, pass);
+
+	if (ret != 0) {
+		printf("ERROR: Failed to load server keys from file %s: %d\n\n",
+				priv_key_fname, ret);
+		goto exit;
+	}
+
+	unsigned char fakecert[2048];
+	ret = x509_encode_fake_cert(&req->rsa.N, &req->rsa.E, fakecert,
+			sizeof(fakecert));
+
+	if (ret <= 0) {
+		printf("ERROR: failed to create fake certificate %d\n", ret);
+		goto exit;
+	}
+
+	ret = x509parse_crt_der(&req->srvcert, fakecert, ret);
+	if (ret != 0) {
+		printf("ERROR: Failed to parse fake certificate returned: %d \n", ret);
+		goto exit;
+	}
+
+	exit: //
+	return ret;
+}
+
+#define DEBUG_LEVEL 0
+
+void my_ssl_debug(void *ctx, int level, const char *str)
+{
+	if (level < DEBUG_LEVEL) {
+		fprintf((FILE *) ctx, "%s", str);
+		fflush((FILE *) ctx);
+	}
+}
+
+volatile int signal_shutdown = 0;
+
+void signal_handler(int s)
+{
+	if (s != 2) {
+		printf("got signal %d, dont know how to process it!\n", s);
+		exit(2);
+	}
+
+	if (signal_shutdown == 0) {
+		fprintf(stderr,
+				"\nGot SIGINT, exiting cleanly (send SIGING one more time to force shutdown)\n");
+		signal_shutdown = 1;
+	} else {
+		fprintf(stderr, "\nGot SIGINT twice, forcing shutdown)\n");
+		exit(1);
+	}
+}
+
+int install_signal_handlers()
+{
+	struct sigaction sig_int_handler;
+
+	sig_int_handler.sa_handler = signal_handler;
+	sigemptyset(&sig_int_handler.sa_mask);
+	sig_int_handler.sa_flags = 0;
+
+	sigaction(SIGINT, &sig_int_handler, NULL );
+
+	return 0;
+}
+
+typedef struct CLIENT_CONNDATA {
+	struct sockaddr_in cl_addr;
+	uint32_t p2p_port;
+} CLIENT_CONNDATA;
+
+/*
+ * connect to this server with:
+ * openssl s_client -tls1_2 -connect localhost:5566
+ * */
+
+int main(void)
+{
+	ssl_prerequisites ssl_req;
+	CLIENT_CONNDATA client;
+
+	int ret = 0;
+	int listen_fd = 0;
+	int port = 5566;
+
+	install_signal_handlers();
+
+	printf("Binding on port %d .. ", port);
+	if ((ret = net_bind(&listen_fd, NULL, port)) != 0) {
+		printf("ERROR\n");
+		goto exit;
+	}
+
+	printf("OK\n");
+
+	printf("Initializing SSL requisites .. ");
+	if ((ret = ssl_requisites_init(&ssl_req)) != 0) {
+		printf("ERROR\n");
+		goto exit;
+	}
+
+	printf("OK\n");
+
+	printf("Switching to blocking mode ... ");
+	if ((ret = net_set_block(listen_fd)) != 0) {
+		printf("ERROR\n");
+		goto exit;
+	}
+
+	printf("OK\n");
+
+	printf("Loading private key ... ");
+	if ((ret = ssl_load_keys(&ssl_req, "./private.der", NULL )) != 0) {
+		printf("ERROR\n");
+		goto exit;
+	}
+
+	printf("OK\n");
+
+	printf("Entering accept loop\n");
+
+	while (!signal_shutdown) {
+		ssl_context ssl;
+		int sin_size = sizeof(struct sockaddr_in);
+		printf("Waiting for connection ... \n");
+		int client_fd = accept(listen_fd, (struct sockaddr*) &client.cl_addr,
+				(socklen_t*) &sin_size);
+
+		if (client_fd <= 0)
+			goto exit;
+
+		printf("Connection from: %s:%d\n", inet_ntoa(client.cl_addr.sin_addr),
+				ntohs(client.cl_addr.sin_port));
+
+		if (net_set_nonblock(client_fd) != 0) {
+			fprintf(stderr, "ERROR: can't set fd to blocking\n");
+			exit(1); // fatal error
+		}
+
+		if ((ret = ssl_init(&ssl)) != 0) {
+			printf(" failed\n  ! ssl_init returned %d\n\n", ret);
+			goto exit;
+		}
+
+		ssl_set_dbg(&ssl, my_ssl_debug, stdout);
+		ssl_set_endpoint(&ssl, SSL_IS_SERVER);
+		ssl_set_authmode(&ssl, SSL_VERIFY_NONE);
+		ssl_set_rng(&ssl, ctr_drbg_random, &ssl_req.ctr_drbg);
+		ssl_set_own_cert(&ssl, &ssl_req.srvcert, &ssl_req.rsa);
+
+		ssl_set_bio(&ssl, net_recv, &client_fd, net_send, &client_fd);
+
+		printf("handshaking... ");
+		while ((ret = ssl_handshake(&ssl)) != 0) {
+			if (ret != POLARSSL_ERR_NET_WANT_READ
+					&& ret != POLARSSL_ERR_NET_WANT_WRITE) {
+				break;
+			}
+		}
+
+		if (ret) {
+			printf("ERROR\n");
+		} else {
+			printf("OK\n");
+
+			char read_buff[512];
+			printf("Starting echo loop\n");
+			while (!signal_shutdown) {
+				int n = ssl_read(&ssl, (unsigned char*) read_buff,
+						sizeof read_buff);
+				if (n == POLARSSL_ERR_NET_WANT_READ)
+					continue;
+
+				// client disconnected
+				if (n == 0)
+					break;
+
+				// some error
+				if (n < 0) {
+					ret = n;
+					break;
+				}
+
+				printf("Received %d bytes\n", n);
+
+				// echoing back buffer
+				while (!signal_shutdown) {
+					n = ssl_write(&ssl, (unsigned char*) read_buff, n);
+
+					if (n == POLARSSL_ERR_NET_WANT_WRITE)
+						continue;
+
+					break;
+				}
+
+				printf("Sent %d bytes back\n", n);
+
+				if (n < 0) {
+					ret = n;
+					break;
+				}
+			}
+		}
+
+		if (ret == 0) {
+			printf("Client disconnected");
+		} else {
+			char errbuff[512];
+			error_strerror(ret, errbuff, sizeof errbuff);
+			printf("Client error: %s\n", errbuff);
+		}
+
+		printf("Cleaning up after disconnect ... ");
+		shutdown(client_fd, SHUT_RDWR);
+		close(client_fd);
+		ssl_free(&ssl);
+		printf("OK\n");
+	}
+
+	exit: //
+	if (ret) {
+		char errbuff[512];
+		error_strerror(ret, errbuff, sizeof errbuff);
+		printf(" - %s\n", errbuff);
+	}
+
+	printf("Exiting.\n");
+
+	ssl_requisites_free(&ssl_req);
+	net_close(listen_fd);
+
+	return 0;
+}
